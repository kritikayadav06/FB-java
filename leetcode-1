class Solution {
    public int[] twoSum(int[] nums, int target) {
     
     int[] n=nums;
     int[] ret= new int[2];
     int t=target;
    for(int i=0;i<n.length-1;i++){
        for(int j=i+1;j<n.length;j++){
            if(n[i]+n[j]==t)
            {
              ret[0]=i;
              ret[1]=j;
              return ret;
            }
        }
    }
     return new int[] {};
        
    }
}
